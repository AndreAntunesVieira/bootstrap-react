{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env"
        ],
        [
          "next/babel"
        ]
      ],
      "plugins": [
        "@babel/plugin-transform-runtime",
        [
          "module-resolver",
          {
            "root": [
              "./"
            ]
          }
        ]
      ]
    },
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./"
            ]
          }
        ]
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./"
            ]
          }
        ]
      ]
    }
  }
}
